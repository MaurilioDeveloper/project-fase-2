  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run browser-sync\" \"npm run sass\" \"npm run watch-css\" ",
    "lite": "lite-server",
    "browser-sync" : "browser-sync start --server --port 8020 --files \"**/*\"",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "sass": "node-sass app/theme/app-theme.scss assets/css/app-theme.css",
    "watch-css": "nodemon -e scss -x npm run sass",
    "postinstall": "\"npm uninstall browser-sync\" \"npm install -g browser-sync\" \"npm uninstall nodemon\" \"npm install -g nodemon\" \"npm uninstall node-sass\" \"npm install -g node-sass\" "
  },
